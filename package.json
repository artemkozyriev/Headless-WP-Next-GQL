    "@faustwp/core": "1.1.2",
    "@glidejs/glide": "^3.6.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@wordpress/base-styles": "^4.26.0",
    "@wordpress/prettier-config": "^3.3.0",
    "adm-zip": "^0.5.10",
